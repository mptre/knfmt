.SUFFIXES: .c .cfake .h .hfake .sh .shfake

.c.cfake:
	sh ${.CURDIR}/knfmt.sh $< ${T}

.h.hfake:
	sh ${.CURDIR}/knfmt.sh $< ${T}

.sh.shfake:
	sh ${.CURDIR}/$<

all: ${TESTS:.c=.cfake}
all: ${TESTS:.h=.hfake}
all: ${TESTS:.sh=.shfake}
